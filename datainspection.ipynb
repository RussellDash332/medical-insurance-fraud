    "train_allprovider.to_csv(r\"D:/Uni Notes/Year 4 Sem 1/DSA4262/data/medical-insurance-fraud/processed_data/train_allprovider.csv\")\n",
    "test_allprovider.to_csv(r\"D:/Uni Notes/Year 4 Sem 1/DSA4262/data/medical-insurance-fraud/processed_data/test_allprovider.csv\")"
   ]
  }
